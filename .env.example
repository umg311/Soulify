# OpenAI API Key - Get this from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Flask Secret Key - Used for session security
# Generate a random key with: python -c "import secrets; print(secrets.token_hex(16))"
FLASK_SECRET_KEY=your_random_secret_key_here

# Set to true in production environment
PRODUCTION=false

# Port to run the application (default is 5000)
PORT=5000

# Server name for production (optional)
# SERVER_NAME=yourdomain.com 